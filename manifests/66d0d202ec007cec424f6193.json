              "type": "Annotation",
              "motivation": "tagging",
              "body": {
                "type": "TextualBody",
                "value": "<p>Badly damaged Reims Cathedral</p>",
                "format": "text/html",
                "language": "en"
              },
              "target": "https://markpbaggett.github.io/corpora-service-maps/66d0d202ec007cec424f6193/canvas/1#xywh=3742,4414,2108,1699"
