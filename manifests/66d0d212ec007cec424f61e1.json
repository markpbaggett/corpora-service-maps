            },
            {
              "id": "https://markpbaggett.github.io/corpora-service-maps/66d0d212ec007cec424f61e1/canvas/1/annotation/34",
              "type": "Annotation",
              "motivation": "tagging",
              "body": {
                "type": "TextualBody",
                "value": "<p>71st Infantry Division's Attached Units listed on a sign held up by cartoon soldiers on both sides. It reads: \"761, 530, 635, 81.\"</p>",
                "format": "text/html",
                "language": "en"
              },
              "target": "https://markpbaggett.github.io/corpora-service-maps/66d0d212ec007cec424f61e1/canvas/1#xywh=7096,407,4488,1175"
